<div class="page-header">
    <h1>New drawing</h1>
</div>

<% form_for(drawing, {action: path_to.drawings(), method: 'POST', id: "drawing_form", class: 'form-horizontal'}, function (form) { %>
    <%- partial('drawings/form.ejs', {locals: {form: form, drawing: drawing}}) %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Create drawing', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.drawings(), {class: 'btn'}) %>
    </div>
<% });%>
