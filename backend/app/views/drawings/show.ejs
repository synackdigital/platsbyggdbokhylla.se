<div class="page-header">
    <h1>Details of drawing</h1>
</div>


<table class="table table-bordered">
    <tbody>
        <tr><th>createDate</th><td><%= drawing.createDate %></td></tr>
        <tr><th>updateDate</th><td><%= drawing.updateDate %></td></tr>
        <tr><th>updateDate</th><td><a href="http://local.platsbyggdbokhylla.se/ritverktyg/#<%= drawing.id %>">Open Drawing</a></td></tr>
        <tr><th>data</th><td><%= drawing.data %></td></tr>    
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', path_to.edit_drawing(drawing), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', path_to.drawing(drawing), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', path_to.drawings()) %>
</div>